
config defaults
	option input 'REJECT'
	option output 'ACCEPT'
	option forward 'REJECT'

config zone
	option name 'GRAVEYARD'
	option input 'REJECT'
	option output 'ACCEPT'
	option forward 'REJECT'
	option masq '1'
	option mtu_fix '1'
	list network 'GRAVEYARD'

config zone
	option name 'NETSEC'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	list network 'NETSEC'

config zone
	option name 'NETSEC-PUBLIC'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	list network 'NETSEC-PUBLIC'

config zone
	option name 'MSSI-LAB'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	list network 'MSSI-LAB'

config forwarding
	option src 'NETSEC-PUBLIC'
	option dest 'GRAVEYARD'
